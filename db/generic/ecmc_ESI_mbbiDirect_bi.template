#-d /**
#-d   \brief template for binary input linked to mbbiDirect for use of ESI converter script 
#-d */

record(bi,"${ECMC_P}${REC_NAME}"){
  field(DESC, "${DESC=}")
  field(PINI, "${PINI=YES}")
  field(INP,  "${ECMC_P}${LNK_NAME}")
  field(ZNAM, "${ZNAM=Zero}")
  field(ONAM, "${ONAM=One}")
  field(ZSV,  "${ZSV=NO_ALARM}")
  field(OSV,  "${OSV=NO_ALARM}")
  field(FLNK, "${FLNK=}}")
  field(TSE,  "$(TSE=-2)")
}
